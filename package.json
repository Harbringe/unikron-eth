{
    "name": "unikron-eth-dex",
    "version": "1.0.0",
    "description": "Ethereum DEX with MEV protection",
    "main": "dist/server.js",
    "scripts": {
        "build": "tsc",
        "dev": "ts-node src/server.ts",
        "dev:multi-dex": "ts-node src/multi-dex-server.ts", 
        "dev:enhanced": "ts-node src/enhanced-dex-server.ts",
        "dev:production": "ts-node src/production-server.ts",
        "start": "node dist/server.js",
        "start:multi-dex": "node dist/multi-dex-server.js",
        "start:enhanced": "node dist/enhanced-dex-server.js",
        "start:production": "node dist/production-server.js",
        "compile": "hardhat compile",
        "deploy": "hardhat run scripts/deploy-production.ts",
        "deploy:sepolia": "hardhat run scripts/deploy-production.ts --network sepolia",
        "deploy:mainnet": "hardhat run scripts/deploy-production.ts --network mainnet",
        "test": "hardhat test",
        "test:coverage": "hardhat coverage"
    },
    "dependencies": {
        "@openzeppelin/contracts": "^5.0.0",
        "axios": "^1.6.0",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "ethers": "^6.8.1",
        "express": "^4.18.2",
        "keccak256": "^1.0.6"
    },
    "devDependencies": {
        "@nomicfoundation/hardhat-ethers": "^3.0.0",
        "@nomicfoundation/hardhat-toolbox": "^4.0.0",
        "@typechain/ethers-v6": "^0.5.0",
        "@typechain/hardhat": "^9.0.0",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/node": "^20.8.10",
        "hardhat": "^2.17.1",
        "ts-node": "^10.9.1",
        "typechain": "^8.3.0",
        "typescript": "^5.2.2"
    },
    "keywords": [
        "ethereum",
        "dex",
        "mev",
        "defi"
    ],
    "author": "Unikron",
    "license": "MIT"
}